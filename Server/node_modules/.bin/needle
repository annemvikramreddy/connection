#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/workspace/reactjs/sessions/authentication-and-authorisation-part-3/Server/node_modules/.pnpm/needle@2.8.0/node_modules/needle/bin/node_modules:/home/workspace/reactjs/sessions/authentication-and-authorisation-part-3/Server/node_modules/.pnpm/needle@2.8.0/node_modules/needle/node_modules:/home/workspace/reactjs/sessions/authentication-and-authorisation-part-3/Server/node_modules/.pnpm/needle@2.8.0/node_modules:/home/workspace/reactjs/sessions/authentication-and-authorisation-part-3/Server/node_modules/.pnpm/node_modules:/home/workspace/reactjs/sessions/authentication-and-authorisation-part-3/Server/node_modules:/home/workspace/reactjs/sessions/authentication-and-authorisation-part-3/node_modules:/home/workspace/reactjs/sessions/node_modules:/home/workspace/reactjs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/reactjs/sessions/authentication-and-authorisation-part-3/Server/node_modules/needle/bin/node_modules:/home/workspace/reactjs/sessions/authentication-and-authorisation-part-3/Server/node_modules/needle/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../needle/bin/needle" "$@"
else
  exec node  "$basedir/../needle/bin/needle" "$@"
fi
