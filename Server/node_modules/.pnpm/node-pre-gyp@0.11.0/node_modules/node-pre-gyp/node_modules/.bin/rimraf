#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/workspace/reactjs/sessions/authentication-and-authorisation-part-3/Server/node_modules/.pnpm/rimraf@2.7.1/node_modules/rimraf/node_modules:/home/workspace/reactjs/sessions/authentication-and-authorisation-part-3/Server/node_modules/.pnpm/rimraf@2.7.1/node_modules:/home/workspace/reactjs/sessions/authentication-and-authorisation-part-3/Server/node_modules/.pnpm/node_modules:/home/workspace/reactjs/sessions/authentication-and-authorisation-part-3/Server/node_modules:/home/workspace/reactjs/sessions/authentication-and-authorisation-part-3/node_modules:/home/workspace/reactjs/sessions/node_modules:/home/workspace/reactjs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/reactjs/sessions/authentication-and-authorisation-part-3/Server/node_modules/.pnpm/node-pre-gyp@0.11.0/node_modules/node-pre-gyp/node_modules:/home/workspace/reactjs/sessions/authentication-and-authorisation-part-3/Server/node_modules/.pnpm/node-pre-gyp@0.11.0/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../rimraf@2.7.1/node_modules/rimraf/bin.js" "$@"
else
  exec node  "$basedir/../../../../../rimraf@2.7.1/node_modules/rimraf/bin.js" "$@"
fi
